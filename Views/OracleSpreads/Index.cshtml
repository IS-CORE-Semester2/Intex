@model IEnumerable<Intex.OracleSpread>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-success m-1">Add New Entry</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BurialSquare)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nors)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sq2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eorw)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Area)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Burialnum)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BurialSquare)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nors)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sq2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eorw)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Burialnum)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Gamous" class="btn btn-secondary m-1" style="width: 80px">Edit</a>
                <a asp-action="Details" asp-route-id="@item.Gamous" class="btn btn-info m-1" style="width: 80px">Details</a>
                <a asp-action="Delete" asp-route-id="@item.Gamous" class="btn btn-danger m-1" style="width: 80px">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
